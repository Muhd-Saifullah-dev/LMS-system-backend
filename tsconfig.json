{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
     "@app/*": ["api/v1/*"],
  "@config/*": ["config/*"],
  "@utils/*": ["utils/*"]
    },
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "nodenext",
    "target": "es2019",
    "types": ["node"],
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
"noEmit": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true
  }
}
